<?xml version="1.0"?>
<extension key="civiauth0jwt" type="module">
  <file>civiauth0jwt</file>
  <name>CiviAuth0Jwt</name>
  <description>CiviCRM extension to allow authorising api calls using JWTs signed by Auth0</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Anthony Blond</author>
    <email>anthony@unfinishedteleporter.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/australiangreens/civiauth0jwt</url>
    <url desc="Documentation">https://github.com/australiangreens/civiauth0jwt</url>
    <url desc="Support">https://github.com/australiangreens/civiauth0jwt</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-07-31</releaseDate>
  <version>1.1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/CiviAuth0Jwt</namespace>
    <format>25.01.1</format>
  </civix>
  <requires>
    <ext>authx</ext>
  </requires>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
  </mixins>
</extension>
